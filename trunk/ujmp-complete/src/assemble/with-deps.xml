				<include>org.ujmp:ujmp-ejml</include>
				<include>ejml:ejml</include>

				<include>org.ujmp:ujmp-hadoop</include>
				<include>hadoop:hadoop-core</include>
				<include>commons-logging:commons-logging</include>

				<include>org.ujmp:ujmp-itext</include>
