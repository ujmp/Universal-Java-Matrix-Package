				<include>derby:derby</include>
				<include>hsqldb:hsqldb</include>

				<include>org.ujmp:ujmp-jexcelapi</include>
				<include>jxl:jxl</include>

				<include>org.ujmp:ujmp-jlinalg</include>
				<include>jlinalg:jlinalg</include>

				<include>org.ujmp:ujmp-jmatharray</include>
				<include>jmatharray:jmatharray</include>

				<include>org.ujmp:ujmp-jmathplot</include>
