				<include>org.ujmp:ujmp-jung</include>
				<include>jung:jung</include>
				<include>commons-collections:commons-collections</include>
				<include>colt:colt</include>
				<include>concurrent:concurrent</include>

				<include>org.ujmp:ujmp-lsimpute</include>
				<include>LSimpute:LSimpute</include>

				<include>org.ujmp:ujmp-lucene</include>
				<include>org.apache.lucene:lucene-core</include>
				<include>org.apache.lucene:lucene-queries</include>

				<include>org.ujmp:ujmp-mail</include>
